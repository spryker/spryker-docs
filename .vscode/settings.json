// Place your settings in this file to overwrite default and user settings.
{
  "files.associations": {
    "*.html": "liquid"
  },
  "spellright.language": [
    "en"
  ],
  "spellright.documentTypes": [
    "latex",
    "plaintext",
    "markdown"
  ],
  "cSpell.words": [
    "Spryker",
    "touchpoints",
    "Ð¡ore"
  ],
  "grammarly.config.documentDialect": "american",
  "grammarly.config.suggestions.OxfordComma": true,
  "editor.renderWhitespace": "all",
  "commentTranslate.targetLanguage": "uk",
  "vale.valeCLI.path": "vale",
  "vale.valeCLI.config": "${workspaceFolder}/.vale.ini",
  "grammarly.config.suggestions.UnnecessaryEllipses": true,
  "grammarly.config.suggestions.Variety": false
}
